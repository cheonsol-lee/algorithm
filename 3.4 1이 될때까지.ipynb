    "N, K = map(int, input().split())\n",
    "count = 0\n",
    "\n",
    "while(True):\n",
    "    # 나머지가 0이면\n",
    "    if N%K == 0:\n",
    "        N = N/K\n",
    "        count += 1\n",
    "    else:\n",
    "        N -= 1\n",
    "        count += 1\n",
    "    if N == 1:\n",
    "        break"
