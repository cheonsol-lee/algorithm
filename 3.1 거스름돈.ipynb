   "source": [
    "def money(N):\n",
    "    count = 0\n",
    "    coin_types = [500, 100, 50, 10]\n",
    "    \n",
    "    for coin in coin_types:\n",
    "        count += N // coin\n",
    "        N = N % coin\n",
    "    return count"
   ]
