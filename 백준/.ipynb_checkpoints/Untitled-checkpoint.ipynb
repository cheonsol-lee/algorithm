   "source": [
    "T = int(input())\n",
    "\n",
    "MAX = 1000001\n",
    "MOD = 1000000009\n",
    "dp = [0] * MAX\n",
    "dp[1] = 1\n",
    "dp[2] = 2\n",
    "dp[3] = 4\n",
    "\n",
    "for i in range(4, MAX):\n",
    "    dp[i] = (dp[i-1] % MOD + dp[i-2] % MOD + dp[i-3] % MOD) % MOD\n",
    "\n",
    "for i in range(T):\n",
    "    n = int(input())\n",
    "    print(dp[n])"
   ]
